{
  "name": "dock-plus",
  "version": "1.0.0",
  "private": false,
  "author": "santiago_fan <san_fan@qq.com>",
  "scripts": {
    "dev": "vitepress dev docs",
    "serve": "vite",
    "build": "vite build --config config/vite.component.js",
    "preview": "vite preview",

    "comp:new": "node build/create-comp.js && node build/build-entry.js",
    "comp:del": "node build/delete-comp.js && node build/build-entry.js",
    
    "theme:build": "gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk",

    "lib:entry": "node build/build-entry.js",
    "lib:clean": "rimraf lib",
    "lib:build": "npm run lib:clean && vite build --config config/vite.component.js && npm run theme:build",
    
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:publish": "node build/publish-docs.js"
  },
  "main": "lib/dock-plus.common.js",
  "dependencies": {
    "element-plus": "^2.10.7",
    "fs-extra": "^11.1.0",
    "qrcode": "1.5.4",
    "vue": "^3.5.16"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/compiler-sfc": "^3.5.16",
    "autoprefixer": "^10.4.23",
    "cp-cli": "^2.0.0",
    "fast-glob": "^3.3.3",
    "gh-pages": "^6.3.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^5.1.0",

    "rimraf": "^3.0.2",
    "sass": "^1.97.2",
    "sass-embedded": "1.89.1",
    "uppercamelcase": "^3.0.0",
    "vite": "^5.4.11",
    "vitepress": "^1.6.4"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
